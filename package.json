    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "mongoose": "^4.3.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-helmet": "^2.2.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.3",

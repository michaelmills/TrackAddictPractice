{
  "name": "TrackAddict",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "find dist -type f -maxdepth 1 -delete",
    "start": "node dist/server.js",
    "dev": "webpack-dev-server --hot --inline --progress --colors --config ./webpack/dev/webpack.dev.config.js",
    "webpack:build": "webpack --progress --colors --config ./webpack/webpack.config.js",
    "build": "npm run clean && npm run webpack:build",
    "build:client": "webpack --progress --colors --config ./webpack/webpack.client.config.js",
    "build:server": "webpack --progress --colors --config ./webpack/webpack.server.config.js",
    "prod": "npm run build && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Michael Mills",
  "license": "ISC",
